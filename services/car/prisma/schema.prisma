generator client {
  provider = "prisma-client-js"
}

datasource carsDb {
  provider = "sqlite"
  url      = "file:./cars.db"
}

model Car {
  id         Int      @id @default(autoincrement())
  brand      String
  model      String
  horsePower Int
  torque     Int
  type       String
//   ownerId    Int?
  createdAt  DateTime @default(now())
//   owner      Customer? @relation(fields: [ownerId], references: [id])
}