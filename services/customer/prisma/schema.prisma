generator client {
  provider = "prisma-client-js"
}

datasource carsDb {
  provider = "sqlite"
  url      = "file:./customers.db"
}

model OwningCar {
  id         Int       @unique
  brand      String
  model      String
  horsePower Int
  torque     Int
  type       String
  createdAt  DateTime  @default(now())
  Customer   Customer? @relation(fields: [customerId], references: [id])
  customerId Int?
}

model Customer {
  id        Int         @id @default(autoincrement())
  name      String
  age       Int
  sex       String
  createdAt DateTime    @default(now())
  cars      OwningCar[]
}
